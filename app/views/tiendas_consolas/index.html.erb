<h1>Listing tiendas_consolas</h1>

<table>
  <thead>
    <tr>
      <th>Cantidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tiendas_consolas.each do |tiendas_consola| %>
      <tr>
        <td><%= tiendas_consola.cantidad %></td>
        <td><%= link_to 'Show', tiendas_consola %></td>
        <td><%= link_to 'Edit', edit_tiendas_consola_path(tiendas_consola) %></td>
        <td><%= link_to 'Destroy', tiendas_consola, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tiendas consola', new_tiendas_consola_path %>
